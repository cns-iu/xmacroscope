{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
    "url": "assets/generated/combined-data.csv",
    "format": {
      "type": "csv",
      "parse": {
        "Time (seconds)": "number",
        "Age Group": null
      }
    }
  },
  "mark": "rect",
  "width": 300,
  "height": 200,
  "encoding": {
    "y": {
      "field": "Age Group",
      "type": "nominal",
      "sort": [
        "",
        "Retired",
        "Adult",
        "Teen",
        "Pre-Teen",
        "Kid"
      ]
    },
    "x": {
      "bin": {"maxbins": 20},
      "field": "Time (seconds)",
      "type": "quantitative"
    },
    "color": {
      "aggregate": "count",
      "type": "quantitative",
      "legend": {
        "title": "Count"
      }
    }
  },
  "config": {
    "view": {
      "stroke": "transparent"
    }
  }
}

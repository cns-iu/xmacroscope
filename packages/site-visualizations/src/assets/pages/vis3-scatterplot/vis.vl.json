{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "url": "assets/generated/combined-data.csv",
    "format": {
      "type": "csv",
      "parse": {
        "Time (seconds)": "number",
        "Age Group": null
      }
    }
  },
  "mark": "point",
  "encoding": {
    "x": {
      "field": "Time (seconds)",
      "type": "quantitative"
    },
    "y": {
      "field": "Age Group",
      "type": "nominal",
      "sort": [
        "",
        "Retired",
        "Adult",
        "Teen",
        "Pre-Teen",
        "Kid"
      ]
    },
    "color": {"field": "Favorite Activity", "type": "nominal"},
    "shape": {"field": "Favorite Activity", "type": "nominal"}
  }
}

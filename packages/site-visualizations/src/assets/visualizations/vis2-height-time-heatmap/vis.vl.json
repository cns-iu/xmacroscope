{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
    "url": "assets/xmac-data/sciencefest-2019/Runs.csv",
    "format": {
      "type": "csv",
      "parse": {
        "Time (seconds)": "number",
        "Age Group": null
      }
    }
  },
  "mark": "rect",
  "width": 300,
  "height": 200,
  "encoding": {
    "y": {
      "bin": {"maxbins":40},
      "field": "Height (inches)",
      "type": "quantitative"
    },
    "x": {
      "bin": {"maxbins": 60},
      "field": "Time (seconds)",
      "type": "quantitative"
    },
    "color": {
      "aggregate": "count",
      "type": "quantitative",
      "legend": {
        "title": "Count"
      }
    }
  },
  "config": {
    "view": {
      "stroke": "transparent"
    }
  }
}
